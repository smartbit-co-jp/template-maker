{
    "name": "smartbit/template-maker",
    "description": "Template maker",
    "keywords": [
        "smartbit",
        "template-maker"
    ],
    "homepage": "https://github.com/smartbit/document-template-editor",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "SB",
            "email": "sb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SmartBit\\TemplateMaker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartBit\\TemplateMaker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartBit\\TemplateMaker\\Providers\\TemplateMakerServiceProvider"
            ],
            "aliases": {
                "TemplateMaker": "SmartBit\\TemplateMaker\\src\\TemplateMakerFacade"
            }
        }
    },
    "minimum-stability": "dev"
}